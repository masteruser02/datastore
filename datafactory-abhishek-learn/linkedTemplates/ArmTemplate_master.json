				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger1_properties_pipeline6_parameters_fname": {
						"value": "[parameters('trigger1_properties_pipeline6_parameters_fname')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactory-abhishek-learn_ArmTemplate_1')]"
			]
		}
	]
}