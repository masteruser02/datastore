		"lsdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlusadf.dfs.core.windows.net/"
		},
		"trigger1_properties_pipeline6_parameters_fname": {
			"type": "string",
			"defaultValue": "triggerbody().filename"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a0a3c3f0-e088-4ef9-9905-565052d4d86f/resourceGroups/adf/providers/Microsoft.Storage/storageAccounts/dlusadf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
